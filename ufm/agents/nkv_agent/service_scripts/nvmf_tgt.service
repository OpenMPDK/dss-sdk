[Unit]
Description=Samsung Object Storage NVMF target operations
After=kv_cli.service

[Service]
User=root
WorkingDirectory=/usr/nkv_agent
Environment="PYTHONPATH=/usr/nkv_agent:/usr/dragonfly/cm/:$PYTHONPATH"
Type=oneshot
ExecStart=/usr/nkv_agent/venv/bin/python kv-cli.py daemon --start_tgt
RemainAfterExit=true
ExecStop=/usr/nkv_agent/venv/bin/python kv-cli.py daemon --stop_tgt

[Install]
WantedBy=multi-user.target

